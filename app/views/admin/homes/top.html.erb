<h3>注文履歴一覧</h3>
<table>
  <tr>
    <th>購入日時</th>
    <th>購入者</th>
    <th>注文個数</th>
  </tr>
  <%= @order_details.each do |order_detail| %>
  <tr>
    <td><%= order_detail.order.created_at.strftime('%Y/%m/%d') %></td>
    <td><%= order_detail.order.receiver_name %></td>
    <td></td>
  </tr>
  <% end %>
</table>
